@{
    ViewData["Title"] = "Home Page";
    Layout = "_Layout";
}

@section Styles {
    <link href="~/css/home.css" rel="stylesheet" />
}


<div class="text-center">
    <h1 class="display-4">Extraordenary rooms with elegant yet a rustic atmosphere</h1>

</div>


@{
    string[] imgs = { "data:image/jpeg;base64,", "data:image/jpeg;base64,", "data:image/jpeg;base64," };
    string[] des = {"Apartment with amazing park view", "Double Deluex Room", "Luxery Apartment"};
    double[] price = { 45.00, 35.00, 40.00 };



}


<div class="container">
    <div class="row">
        @for (int i = 0; i < imgs.Length; i++)
        {
            <div class="col-md-4 d-flex">

                <div class="card" style="width: 18rem;">
                    <img class="card-img-top" src="@imgs[i]" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">@des[i]</h5>
                        <p class="card-text">$@price[i].00</p>
                        <a href="#" class="view">View</a>
                    </div>
                </div>
            </div>
        }
    </div>
</div>



<footer class="bg-body-tertiary text-center text-lg-start" style="margin-top: 100px;">
    <!-- Copyright -->
    <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.05);">
        © 2025 Copyright:
        <a class="text-body" href="https://mdbootstrap.com/">MDBootstrap.com</a>
    </div>
    <!-- Copyright -->
</footer>